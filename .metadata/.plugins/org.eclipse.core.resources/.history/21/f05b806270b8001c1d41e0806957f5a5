server:
  port: 8000
spring:
  application:
    name: cambio-service
  jpa:
    show-sql: true
    hibernate:
      ddl-auto: update
    properties:
      hibernate:
      #dialect: org.hibernate.dialect.MySQL8Dialect
      dialect: org.hibernate.dialect.MySQL5InnoDBDialect
  jmx:
    enabled: false
  datasource:
    url: jdbc:mysql://localhost:3306/cambio
    username: root
    password: root
  flyway:
    url: jdbc:mysql://localhost:3306/
    schemas: cambio
    user: root
    password: root